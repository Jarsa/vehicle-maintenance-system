        <field eval="1" name="number_increment" />
        <field name="padding">5</field>
    </record>
    <record id="b2c_report_sequence" model="ir.sequence">
        <field name="name">B2C Report Sequence</field>
        <field name="prefix">R-B2C-</field>
        <field eval="1" name="number_next" />
        <field eval="1" name="number_increment" />
        <field name="padding">5</field>
    </record>
</odoo>
