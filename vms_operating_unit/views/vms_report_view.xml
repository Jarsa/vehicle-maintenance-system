<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="vms.report_view_form" model="ir.ui.view">
        <field name="name">vms.report.view.form</field>
        <field name="model">vms.report</field>
        <field name="inherit_id" ref="vms.vms_report_form_view" />
        <field name="arch" type="xml">
            <xpath
                expr="//group[@name='details']/field[@name='date']"
                position="before"
            >
                <field
                    name="operating_unit_id"
                    groups="operating_unit.group_multi_operating_unit"
                />
            </xpath>
        </field>
    </record>
</odoo>
